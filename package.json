{
  "name": "dapp-scaffold",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@craco/craco": "^5.7.0",
    "@ledgerhq/hw-transport-webusb": "^5.41.0",
    "@project-serum/anchor": "0.6.1-beta.1",
    "@project-serum/common": "^0.0.1-beta.3",
    "@project-serum/serum": "^0.13.34",
    "@project-serum/sol-wallet-adapter": "^0.2.0",
    "@solana/spl-token": "0.1.3",
    "@solana/spl-token-registry": "^0.2.64",
    "@solana/spl-token-swap": "0.1.0",
    "@solana/web3.js": "^1.7.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/react-router-dom": "^5.1.6",
    "antd": "^4.6.6",
    "bn.js": "^5.1.3",
    "borsh": "^0.4.0",
    "bs58": "^4.0.1",
    "buffer-layout": "^1.2.0",
    "craco-less": "^1.17.0",
    "eventemitter3": "^4.0.7",
    "jazzicon": "^1.5.0",
    "mz": "^2.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "yaml": "^1.10.2"
  },
  "scripts": {
    "start": "cd contract && anchor launch --program-name circlepod_protocol",
    "start:devdapp": "ts-node contract/client/main.ts",
    "start:devdapp:client": "ts-node contract/client/client.ts",
    "clean": "cd contract && rm -rf target/idl target/release target/deploy",
    "build:anchor": "cd contract && anchor build",
    "build:anchor:clean": "yarn clean && cd contract && anchor build",
    "deploy:anchor": "cd contract && anchor deploy --program-name circlepod_protocol",
    "deploy:solana": "solana program deploy contract/target/deploy/circlepod_protocol.so",
    "test": "cd contract && anchor test --skip-build",
    "eject": "react-scripts eject",
    "localnet:logs": "solana logs",
    "predeploy": "git pull --ff-only && yarn && yarn build",
    "format:fix": "prettier --write \"**/*.+(js|jsx|ts|tsx|json|css|md)\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solana-labs/dapp-scaffold"
  },
  "homepage": ".",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/bn.js": "^4.11.6",
    "@types/bs58": "^4.0.1",
    "@types/eslint": "^7.2.4",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jest": "^24.9.1",
    "@types/ledgerhq__hw-transport": "^4.21.3",
    "@types/ledgerhq__hw-transport-webusb": "^4.70.1",
    "@types/mz": "^2.7.2",
    "@types/node": "^15.6.1",
    "@types/prettier": "^2.1.5",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/yaml": "^1.9.7",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "arweave-deploy": "^1.9.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "mocha": "^8.4.0",
    "prettier": "^2.1.2",
    "start-server-and-test": "^1.11.6",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
